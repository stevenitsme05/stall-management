			$('#birthdate').val() != '' ||
			$('#birthplace').val() != '' ||
			$('#fatherfullname').val() != '' ||
			$('#fatherbirthplace').val() != '' ||
			$('#motherfullname').val() != '' ||
			$('#motherbirthplace').val() != '' ||
			$('#relation').val() != '' ||
			$('#contactnumber').val() != '' ||
			$('.marriageaddress').val() != '' ||
			$('.address').val() != '' ||
			$('#file-0b').val() != '' ||
			$('#file-0b').val() != undefined){
				$('#nextButton2').addClass('btn-next')
				$('#nextButton2').click();
				
			}
			else{
