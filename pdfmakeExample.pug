		});
		$('#exampleRadios1').click(function() {
			$('#textarea1').slideDown();
			$('#textarea2').slideUp();
		});
		$('#exampleRadios3').click(function() {
			$('#textarea1').slideUp();
			$('#textarea2').slideDown();
		});
		$('#regular').hide();
		$('#special').hide();
		
		if(eventid == 3) {
			
			baptismType1 = "Regular";
			console.log(baptismType1)
			$('#regular').slideDown();
			$('#special').slideUp();
		}
		if(eventid == 3){
		$('#nextButton2').click(function(){
		$.get('/guest/baptism/query').done(data=>{
		console.log($("#baptismtype").val())
		console.log(data.utilities[0].int_agemax)
		console.log(baptismType1)
			console.log($('#file-0b').val())
			if ($("#customRadio1").prop("checked")) {
			gender = $("#customRadio1").val();
			}
			else if($("#customRadio2").prop("checked")){
			gender = $("#customRadio2").val();
			}
			console.log(gender)
			var age = moment().diff($('#birthdate').val(),'years')
			console.log(age)
			if(
			gender != undefined &&
			$('#firstname').val() != ''  &&
			$('#lastname').val() != ''  &&
			$('#birthdate').val() != '' &&
			$('#birthplace').val() != '' &&
			$('#fatherfullname').val() != '' &&
