block addjs
	script.
		var baptismType1 = "";
		var eventid =$("#eventid").val();
		console.log($("#eventid").val())
		$('#textarea1').hide();
		$('#textarea2').hide();
		$('#radioup').click(function() {
			$('#textarea1').slideUp();
